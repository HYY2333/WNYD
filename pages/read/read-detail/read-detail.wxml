<!--pages/read/read-detail/read-detail.wxml-->
<view class='list'>
  <view class='title'>
    {{detailD.tit}}
  </view>
  <view class='author'>
    <image src='{{detailD.aPhoto}}'></image>
    <text>{{detailD.author}}</text>
  </view>
  <view class='intro'>
    {{detailD.words}}
  </view>
  <image class='big-pic' src='{{detailD.bigImg}}' mode= "aspectFill"></image>
</view>